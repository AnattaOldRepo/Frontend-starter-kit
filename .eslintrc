{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["babel"],
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "globals": {
    "Promise": true
  },
  "rules": {
    "indent": [2, "tab", {"SwitchCase": 1}],
    "comma-dangle": 0,
    "no-console": 0
  }
}
